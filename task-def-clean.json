{
  "family": "music-player-dev",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "music-player",
      "image": "113221193811.dkr.ecr.ap-southeast-1.amazonaws.com/music-player-dev:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NEXTAUTH_URL",
          "value": "http://music-player-alb-dev-681721043.ap-southeast-1.elb.amazonaws.com"
        }
      ],
      "secrets": [
        {
          "name": "POSTGRES_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:113221193811:secret:database-url-09H0tc"
        },
        {
          "name": "NEXTAUTH_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:113221193811:secret:nextauth-secret-w4FbnN"
        },
        {
          "name": "BLOB_READ_WRITE_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:113221193811:secret:blob-token-gBcd1N"
        },
        {
          "name": "OPENAI_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:113221193811:secret:openai-key-zxgWTO"
        },
        {
          "name": "SUPABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:113221193811:secret:supabase-url-fMWwYv"
        },
        {
          "name": "SUPABASE_ANON_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:113221193811:secret:supabase-anon-key-0EADEN"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/music-player-dev",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "wget --no-verbose --tries=1 --spider http://localhost:3000/ || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ],
  "taskRoleArn": "arn:aws:iam::113221193811:role/music-player-ecs-task-role-dev",
  "executionRoleArn": "arn:aws:iam::113221193811:role/music-player-ecs-task-execution-role-dev"
}
